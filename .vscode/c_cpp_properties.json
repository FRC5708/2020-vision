{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/local/include/opencv4"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.14.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64",
            "configurationProvider": "vscode-wpilib"
        },
        {
            "name": "Raspberry Pi w/ Mac FRC gcc",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../pi_include/usr/include/",
                "${workspaceFolder}/../pi_include/usr/include/**",
                "${workspaceFolder}/../pi_include/opt/vc/include/",
                "${workspaceFolder}/../pi_include/opt/vc/include/**"
            ],
            "compilerPath": "~/wpilib/2020/roborio/bin/arm-frc2020-linux-gnueabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-mcpu=cortex-m4",
				"-mthumb",
				"-mfloat-abi=hard"
            ],
            "defines": [
                "__ARM_PCS_VFP"
            ]
        },
        {
            "name": "WSL",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}